# Wrangler Configuration Template
# Copy to wrangler.toml and fill in your values
# DO NOT commit wrangler.toml (contains account_id)

name = "og-image"
compatibility_date = "2024-01-01"
pages_build_output_dir = "out"

# Account ID - use environment variable CLOUDFLARE_ACCOUNT_ID instead
# account_id = "your-account-id-here"

# Build configuration
[build]
command = "npm run build"

# Environment-specific settings
[env.production]
name = "og-image"

[env.preview]
name = "og-image-preview"
